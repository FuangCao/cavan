<packages>
	<package name="bash" version="4.1" depend="" config="--prefix=/"
	         url="http://ftp.gnu.org/gnu/bash">
	</package>

	<package name="zlib" version="1.2.5" depend=""
	         url="http://ncu.dl.sourceforge.net/project/libpng/zlib/$(ZLIB_VERSION)"
			 config="--prefix=/usr">
	</package>

	<package name="ncurses" version="5.9" depend=""
			 url="http://ftp.gnu.org/gnu/ncurses"
	         config="--prefix=/usr">
	</package>

	<package name="acl" version="2.2.49" depend="attr"
			 url="http://mirrors.zerg.biz/nongnu/acl/$(ACL_NAME).src.tar.gz"
	         config="--prefix=/usr">
	</package>

	<package name="attr" version="2.4.46" depend=""
			 url="http://mirrors.zerg.biz/nongnu/attr/$(ATTR_NAME).src.tar.gz"
	         config="--prefix=/usr">
	</package>

	<package name="libffi" version="3.0.9" depend=""
			 url="ftp://sourceware.org/pub/libffi"
	         config="--prefix=/usr">
	</package>

	<package name="libusb" version="0.1.12" depend=""
			 url="http://ncu.dl.sourceforge.net/project/libusb/libusb-0.1%20%28LEGACY%29/0.1.12/libusb-0.1.12.tar.gz"
	         config="--prefix=/usr">
	</package>

	<package name="usbutils" version="0.84" depend="libusb"
	         url="http://down1.chinaunix.net/distfiles"
			 config="--prefix=/usr">
	</package>

	<package name="glib" version="2.30.0" depend=""
			 url="http://ftp.gnome.org/pub/gnome/sources/glib/2.30"
	         config="--prefix=/usr CFLAGS='-I$(SYSROOT_PATH)/usr/include'">
	</package>
</packages>
