#!/bin/sh

"""exec" python -E $0 $@
#"""

import sys, os

from python import git_svn
from python.cavan_stdio import pr_red_info, pr_green_info

if __name__ == "__main__":
	if len(sys.argv) > 1:
		manager = git_svn.GitSvnManager()
		if manager.main(sys.argv):
			pr_green_info("OK")
			sys.exit(0)

	pr_red_info("Failed")
	sys.exit(-1)
