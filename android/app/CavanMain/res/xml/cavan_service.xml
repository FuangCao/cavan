<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
    android:key="parent">

	<Preference android:key="ip_address"
	    android:title="@string/text_ip_address"
	    android:summary="@string/text_unknown" />

	<Preference android:key="message_show"
	    android:title="@string/text_message_show" />

	<CheckBoxPreference android:key="auto_unlock"
	    android:persistent="true"
	    android:title="@string/text_auto_unlock" />

	<CheckBoxPreference android:key="auto_commit"
	    android:persistent="true"
	    android:title="@string/text_auto_commit" />

	<CheckBoxPreference android:key="auto_unpack"
	    android:title="@string/text_auto_unpack" />

	<CheckBoxPreference android:key="listen_clip"
	    android:persistent="true"
	    android:title="@string/text_listen_clip" />

	<Preference android:key="input_method_select"
	    android:title="@string/text_input_method_select" />
1
	<RingtonePreference android:key="red_packet_notify_ringtone"
	    android:persistent="true"
	    android:title="@string/red_packet_notify_ringtone" />

	<EditTextPreference android:key="red_packet_notify_test"
	    android:persistent="true"
	    android:title="@string/red_packet_notify_test" />

	<Preference android:key="permission_settings"
	    android:title="@string/text_permission_settings" />

	<CheckBoxPreference android:key="float_timer"
	    android:persistent="true"
	    android:title="@string/desktop_float_timer" />

	<com.cavan.cavanjni.TcpDdServicePreference android:key="tcp_dd"
	    android:persistent="true"
	    android:title="@string/tcp_dd_service" />

	<com.cavan.cavanjni.FtpServicePreference android:key="ftp"
	    android:persistent="true"
	    android:title="@string/ftp_service" />

	<com.cavan.cavanjni.WebProxyServicePreference android:key="web_proxy"
	    android:persistent="true"
	    android:title="@string/web_proxy_service" />

