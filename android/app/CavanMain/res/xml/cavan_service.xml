<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
    android:key="parent">

	<Preference
	    android:key="ip_address"
	    android:title="@string/text_ip_address"
	    android:summary="@string/text_unknown"/>

	<com.cavan.cavanutils.CavanServicePreference
	    android:key="tcp_dd_service"
	    android:persistent="true"
	    android:title="@string/tcp_dd_service"/>

	<com.cavan.cavanutils.CavanServicePreference
	    android:key="ftp_service"
	    android:persistent="true"
	    android:title="@string/ftp_service"/>

	<com.cavan.cavanutils.CavanServicePreference
	    android:key="web_proxy_service"
	    android:persistent="true"
	    android:title="@string/web_proxy_service"/>

</PreferenceScreen>
